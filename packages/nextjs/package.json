{
  "name": "@se-2/nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "npx http-server storybook-static -p 6006",
    "storybook:capture": "node scripts/capture-story.mjs",
    "build:analyze": "ANALYZE=true NEXT_TELEMETRY_DISABLED=1 next build",
    "check-types": "tsc --noEmit --incremental",
    "dev": "next dev --turbo",
    "format": "prettier --write . '!(node_modules|.next|contracts)/**/*'",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 1000",
    "lint:perf": "eslint . --ext .ts,.tsx --rule 'react-perf/jsx-no-new-object-as-prop: error' --rule 'react-perf/jsx-no-new-array-as-prop: error' --rule 'react-perf/jsx-no-new-function-as-prop: error'",
    "serve": "next start",
    "start": "next dev --turbo",
    "vercel": "vercel --build-env YARN_ENABLE_IMMUTABLE_INSTALLS=false --build-env ENABLE_EXPERIMENTAL_COREPACK=1 --build-env VERCEL_TELEMETRY_DISABLED=1",
    "vercel:login": "vercel login",
    "vercel:yolo": "vercel --build-env YARN_ENABLE_IMMUTABLE_INSTALLS=false --build-env ENABLE_EXPERIMENTAL_COREPACK=1 --build-env NEXT_PUBLIC_IGNORE_BUILD_ERROR=true --build-env VERCEL_TELEMETRY_DISABLED=1",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "webhooks:setup": "npx tsx --env-file=.env scripts/setup-alchemy-webhooks.ts",
    "webhook:test": "npx tsx scripts/test-webhook.ts"
  },
  "dependencies": {
    "@avnu/avnu-sdk": "3.1.1",
    "@cartridge/connector": "^0.10.7",
    "@heroicons/react": "~2.1.5",
    "@radix-ui/themes": "^3.2.1",
    "@rainbow-me/rainbowkit": "2.1.6",
    "@scaffold-stark/stark-burner": "^0.1.13",
    "@starknet-react/chains": "^5.0.3",
    "@starknet-react/core": "^5.0.3",
    "@tanstack/react-query": "^5.72.2",
    "@tanstack/react-table": "^8.21.3",
    "@uniswap/sdk-core": "~5.8.2",
    "@uniswap/v2-sdk": "~4.6.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "abi-wan-kanabi": "^2.2.4",
    "alchemy-sdk": "^3.6.5",
    "blo": "~1.2.0",
    "burner-connector": "0.0.9",
    "clsx": "^2.1.1",
    "daisyui": "4.12.10",
    "drizzle-orm": "^0.45.1",
    "ethers": "^6.12.0",
    "framer-motion": "^12.4.10",
    "gray-matter": "^4.0.3",
    "json-stringify-deterministic": "^1.0.12",
    "lodash.merge": "^4.6.2",
    "lucide-react": "^0.553.0",
    "moment": "^2.30.1",
    "next": "^16.1.1",
    "next-mdx-remote": "^5.0.0",
    "next-nprogress-bar": "~2.3.13",
    "next-themes": "~0.3.0",
    "postgres": "^3.4.8",
    "qrcode.react": "~4.0.1",
    "qs": "^6.13.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "sonner": "^2.0.7",
    "starknet": "^8.5.4",
    "starknetkit": "3.0.3",
    "usehooks-ts": "~3.1.0",
    "viem": "^2.26.3",
    "vitest": "^3.1.1",
    "wagmi": "^2.14.16",
    "zod": "^3.24.2",
    "zustand": "~5.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.1",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/react": "^8.5.0",
    "@storybook/react-vite": "^8.5.0",
    "@storybook/test": "^8.5.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@trivago/prettier-plugin-sort-imports": "~4.3.0",
    "@types/eslint-plugin-tailwindcss": "^3",
    "@types/jsdom": "^27",
    "@types/lodash.merge": "^4",
    "@types/node": "~18.19.50",
    "@types/react": "^19.2.1",
    "@types/react-dom": "^19.2.1",
    "@typescript-eslint/eslint-plugin": "~5.40.0",
    "@vitejs/plugin-react": "^5.1.2",
    "abitype": "1.0.6",
    "autoprefixer": "~10.4.20",
    "babel-plugin-react-compiler": "latest",
    "drizzle-kit": "^0.31.8",
    "eslint": "~8.57.1",
    "eslint-config-next": "15",
    "eslint-plugin-react-perf": "^3.3.3",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-tailwindcss": "3.14.0",
    "jsdom": "^27.4.0",
    "playwright": "^1.57.0",
    "postcss": "~8.4.45",
    "prettier": "~3.3.3",
    "storybook": "^8.5.0",
    "tailwindcss": "~3.4.11",
    "type-fest": "~4.26.1",
    "typescript": "^5.7.0",
    "vercel": "~39.1.3"
  },
  "packageManager": "yarn@3.2.3"
}
